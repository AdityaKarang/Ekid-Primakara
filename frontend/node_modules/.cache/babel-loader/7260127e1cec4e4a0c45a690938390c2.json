\r\n          <v-toolbar flat>\r\n            <v-icon>mdi-file</v-icon>\r\n            <v-toolbar-title class=\"font-weight-light\">\r\n              Tambah Bidang Fokus\r\n            </v-toolbar-title>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"white\" fab small @click=\"showAdd = false\">\r\n              <v-icon> mdi-close </v-icon>\r\n            </v-btn>\r\n          </v-toolbar>\r\n          <v-divider></v-divider>\r\n          <v-card-text>\r\n            <v-form>\r\n              <v-row>\r\n                <v-col md=\"6\">\r\n                  <v-text-field v-model=\"form.bidang_fokus\" label=\"Bidang Fokus\" required></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n              <b-button type=\"button\" class=\"mr-2\" variant=\"primary\" @click=\"confirmasi = true\">Save</b-button>\r\n            </v-form>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-dialog>\r\n      <v-dialog v-model=\"showEdit\" max-width=\"400px\">\r\n        <v-card class=\"\">\r\n          <v-toolbar flat>\r\n            <v-icon>mdi-file</v-icon>\r\n            <v-toolbar-title class=\"font-weight-light\">\r\n              Edit Bidang Fokus\r\n            </v-toolbar-title>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"white\" fab small @click=\"showEdit = false\">\r\n              <v-icon> mdi-close </v-icon>\r\n            </v-btn>\r\n          </v-toolbar>\r\n          <v-divider></v-divider>\r\n          <v-card-text>\r\n            <v-form>\r\n              <v-row>\r\n                <v-col md=\"6\">\r\n                  <v-text-field v-model=\"form.bidang_fokus\" label=\"Bidang Fokus\" required></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n              <b-button type=\"button\" class=\"mr-2\" variant=\"primary\" @click=\"onSave()\">Save</b-button>\r\n            </v-form>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-dialog>\r\n      <CustomCard title=\"Daftar Bidang Fokus\" deskripsi=\"List Daftar Bidang Fokus Yang Telah Di Inputkan\"\r\n        :headers=\"headers\" :contentTable=\"contentTables\" :isDetail=\"false\" @detailItem=\"detailData\" @editItem=\"editData\"\r\n        @deleteItem=\"deleteData\" @addNew=\"addNew\" />\r\n    </div>\r\n  </b-card>\r\n</template>\r\n<script>\r\nimport { SET_BREADCRUMB } from \"@/core/services/store/breadcrumbs.module\";\r\nimport CustomCard from \"@/view/content/CustomCard\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"bidang_fokuses\",\r\n  components: {\r\n    CustomCard,\r\n  },\r\n  data() {\r\n    return {\r\n      form: {\r\n        bidang_fokuses: [],\r\n      },\r\n      semesters: [{ text: \"Select One\", value: null }, \"Genap\", \"Ganjil\"],\r\n      showEdit: false,\r\n      showAdd: false,\r\n      confirmasi: false,\r\n      confirmasiDelete: false,\r\n      file: null,\r\n      contentTables: [],\r\n      headers: [\r\n        { text: \"No\", value: \"no\" },\r\n        { text: \"Bidang Fokus\", value: \"bidang_fokus\" },\r\n        { text: \"Actions\", value: \"detail\" },\r\n      ],\r\n    };\r\n  },\r\n  computed:{\r\n    currentUser(){\r\n      return this.$store.state.auth.user.data\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getbidang_fokuses();\r\n    console.log(this.currentUser);\r\n  },\r\n  methods: {\r\n    getbidang_fokuses() {\r\n      axios.get('http://localhost:8000/api/bidangfokus').then(res => {\r\n        this.contentTables = res.data.data.map((bf, index) => {\r\n          return {\r\n            no: index + 1,\r\n            bidang_fokus: bf.bidang_fokus,\r\n            detail: bf.id\r\n          }\r\n        })\r\n        console.log()\r\n      })\r\n    },\r\n    addNew(value) {\r\n      this.showAdd = value;\r\n    },\r\n    onClick() {\r\n      axios.post('http://localhost:8000/api/bidangfokus', {\r\n        bidang_fokus: this.form.bidang_fokus,\r\n      })\r\n        .then(response => {\r\n          // this.confirmasi = true;\r\n          this.getbidang_fokuses();\r\n          this.form.bidang_fokus = '';\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    detailData(item) {\r\n      console.log(\"detailData\", item);\r\n    },\r\n\r\n  editData(item) {\r\n  console.log(item); \r\n  this.form = { bidang_fokus: item.bidang_fokus };\r\n  this.showEdit = true; \r\n  this.confirmasi = false;\r\n  this.editItemId = item.detail; // Set the id of the item being edited\r\n\r\n  // Remove the axios.put call here\r\n},\r\nonSave() {\r\n  axios.put(`http://localhost:8000/api/bidangfokus/${this.editItemId}`, {\r\n    bidang_fokus: this.form.bidang_fokus\r\n  }).then(res => {\r\n    console.log(res.data);\r\n    this.getbidang_fokuses();\r\n    this.showEdit = false;\r\n  }).catch(error => {\r\n    console.error(error);\r\n  });\r\n},\r\n\r\ndeleteData(item) {\r\n  console.log(item); \r\n  this.confirmasiDelete = true;\r\n  this.deleteId = item.detail; // Set the id of the item being edited\r\n\r\n},\r\n    onDelete() {\r\n  axios.delete(`http://localhost:8000/api/bidangfokus/${this.deleteId}`)\r\n    .then(response => {\r\n      console.log(response.data);\r\n      this.confirmasiDelete = false;\r\n      this.getbidang_fokuses();\r\n    })\r\n    .catch(error => {\r\n      console.error(error);\r\n      this.confirmasiDelete = false;\r\n    });\r\n}\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<!-- import { SET_BREADCRUMB } from \"@/core/services/store/breadcrumbs.module\";\r\nimport CustomCard from \"@/view/content/CustomCard\";\r\nexport default {\r\n  name: \"Bidangfokus\",\r\n  components: {\r\n    CustomCard,\r\n  },\r\n  data() {\r\n    return {\r\n      form: {\r\n        bidangfokus: \"\",\r\n      },\r\n\r\n      semesters: [{ text: \"Select One\", value: null }, \"Genap\", \"Ganjil\"],\r\n      showAdd: false,\r\n      confirmasi: false,\r\n      confirmasiDelete: false,\r\n\r\n      file: null,\r\n      headers: [\r\n        { text: \"No\", value: \"no\" },\r\n        { text: \"Bidang Fokus\", value: \"bidangfokus\" },\r\n        { text: \"Actions\", value: \"detail\" },\r\n      ],\r\n      contentTables: []\r\n      \r\n    };\r\n  },\r\n  methods: {\r\n    onSubmit(evt) {\r\n      evt.preventDefault();\r\n      alert(JSON.stringify(this.form));\r\n    },\r\n    onReset(evt) {\r\n      evt.preventDefault();\r\n      // Reset our form values\r\n      // Trick to reset/clear native browser form validation state\r\n      this.show = false;\r\n      this.$nextTick(() => {\r\n        this.show = true;\r\n      });\r\n    },\r\n    detailData(item) {\r\n      this.$router.push({\r\n        name: \"detail-penelitian\",\r\n        params: { data: item },\r\n      });\r\n    },\r\n    editData(item) {\r\n      console.log(\"test\");\r\n      this.showAdd = true;\r\n      this.form.bidangfokus = item.bidangfokus;\r\n    },\r\n    deleteData(item) {\r\n      this.confirmasiDelete = true;\r\n    },\r\n    addNew(value) {\r\n      this.showAdd = value;\r\n    },\r\n    onClick() {\r\n      this.confirmasi = true;\r\n    },\r\n\r\n    remove(item, data) {\r\n      const index = data.indexOf(item.nama);\r\n      if (index >= 0) data.splice(index, 1);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$store.dispatch(SET_BREADCRUMB, [{ title: \"Master Data\" }]);\r\n  },\r\n}; -->"],"sourceRoot":"src/view/lppm/pages"}]}