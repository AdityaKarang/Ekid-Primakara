,gDAAA,KAAA,QAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,eAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;AACA,OATA;AAUA,KA5EA;AA6EA,IAAA,MA7EA,kBA6EA,IA7EA,EA6EA,IA7EA,EA6EA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,IAAA,CAAA,EAAA,IAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AAhFA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <b-card>\r\n    <div class=\"mt-3\">\r\n      <v-row justify=\"center\">\r\n        <v-dialog v-model=\"confirmasi\" max-width=\"300\">\r\n          <v-card>\r\n            <v-card-title class=\"headline\">Apakah Anda Yakin Ingin Menyimpan Data?</v-card-title>\r\n\r\n            <v-card-text> pastikan anda mengis data dengan benar </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"confirmasi = false\">\r\n                Cancel\r\n              </v-btn>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"onClick();\r\n              confirmasi = false;\r\n              showAdd = false;\r\n              \">\r\n                Save\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n        <v-dialog v-model=\"confirmasiDelete\" max-width=\"300\">\r\n          <v-card>\r\n            <v-card-title class=\"headline\">Apakah Anda Yakin Menghapus data?</v-card-title>\r\n\r\n            <v-card-text>\r\n              pastikan anda menghapus data dengan benar\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"confirmasiDelete = false\">\r\n                Cancel\r\n              </v-btn>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"onDelete();\r\n              confirmasiDelete = false;\r\n              showAdd = false;\r\n              \">\r\n                Hapus\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-row>\r\n      <v-dialog v-model=\"showAdd\" max-width=\"400px\">\r\n        <v-card class=\"\">\r\n          <v-toolbar flat>\r\n            <v-icon>mdi-file</v-icon>\r\n            <v-toolbar-title class=\"font-weight-light\">\r\n              Tambah Bentuk Dana\r\n            </v-toolbar-title>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"white\" fab small @click=\"showAdd = false\">\r\n              <v-icon> mdi-close </v-icon>\r\n            </v-btn>\r\n          </v-toolbar>\r\n          <v-divider></v-divider>\r\n          <v-card-text>\r\n            <v-form>\r\n              <v-row>\r\n                <v-col md=\"6\">\r\n                  <v-text-field v-model=\"form.bentuk_dana\" label=\"Bentuk Dana\" required></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n              <b-button type=\"button\" class=\"mr-2\" variant=\"primary\" @click=\"confirmasi = true\">Save</b-button>\r\n            </v-form>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-dialog>\r\n      <v-dialog v-model=\"showEdit\" max-width=\"400px\">\r\n        <v-card class=\"\">\r\n          <v-toolbar flat>\r\n            <v-icon>mdi-file</v-icon>\r\n            <v-toolbar-title class=\"font-weight-light\">\r\n              Edit Bentuk Dana\r\n            </v-toolbar-title>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"white\" fab small @click=\"showEdit = false\">\r\n              <v-icon> mdi-close </v-icon>\r\n            </v-btn>\r\n          </v-toolbar>\r\n          <v-divider></v-divider>\r\n          <v-card-text>\r\n            <v-form>\r\n              <v-row>\r\n                <v-col md=\"6\">\r\n                  <v-text-field v-model=\"form.bentuk_dana\" label=\"Bentuk Dana\" required></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n              <b-button type=\"button\" class=\"mr-2\" variant=\"primary\" @click=\"onSave()\">Save</b-button>\r\n            </v-form>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-dialog>\r\n      <CustomCard title=\"Daftar Bentuk Dana\" deskripsi=\"List Daftar Bentuk Dana Yang Telah Di Inputkan\" :headers=\"headers\"\r\n        :contentTable=\"contentTables\" :isDetail=\"false\" @detailItem=\"detailData\" @editItem=\"editData\"\r\n        @deleteItem=\"deleteData\" @addNew=\"addNew\" />\r\n    </div>\r\n  </b-card>\r\n</template>\r\n<script>\r\nimport { SET_BREADCRUMB } from \"@/core/services/store/breadcrumbs.module\";\r\nimport CustomCard from \"@/view/content/CustomCard\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Bentukdana\",\r\n  components: {\r\n    CustomCard,\r\n  },\r\n  data() {\r\n    return {\r\n      form: {\r\n        bentuk_danas: [],\r\n      },\r\n\r\n      semesters: [{ text: \"Select One\", value: null }, \"Genap\", \"Ganjil\"],\r\n      showEdit: false,\r\n      showAdd: false,\r\n      confirmasi: false,\r\n      confirmasiDelete: false,\r\n      contentTables: [],\r\n      file: null,\r\n      headers: [\r\n        { text: \"No\", value: \"no\" },\r\n        { text: \"Bentuk Dana\", value: \"bentuk_dana\" },\r\n        { text: \"Actions\", value: \"detail\" },\r\n      ],\r\n    };\r\n  },\r\n  computed:{\r\n    currentUser(){\r\n      return this.$store.state.auth.user.data\r\n      \r\n    }\r\n  },\r\n  mounted() {\r\n    this.getbentuk_danas();\r\n    console.log(this.currentUser);\r\n  },\r\n  methods: {\r\n    getbentuk_danas() {\r\n      axios.get('http://localhost:8000/api/bentukdana').then(res => {\r\n        this.contentTables = res.data.data.map((bf, index) => {\r\n          return {\r\n            no: index + 1,\r\n            bentuk_dana: bf.bentuk_dana,\r\n            detail: bf.id\r\n          }\r\n        })\r\n        console.log()\r\n      })\r\n    },\r\n\r\n    addNew(value) {\r\n      this.showAdd = value;\r\n    },\r\n    onClick() {\r\n      axios.post('http://localhost:8000/api/bentukdana', {\r\n        bentuk_dana: this.form.bentuk_dana,\r\n      })\r\n        .then(response => {\r\n          // this.confirmasi = true;\r\n          console.log(response.data);\r\n          this.getbentuk_danas();\r\n          this.form.bentuk_dana = '';\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    detailData(item) {\r\n      this.$router.push({\r\n        name: \"detail-penelitian\",\r\n        params: { data: item },\r\n      });\r\n    },\r\n    editData(item) {\r\n      console.log(item);\r\n      this.form = { bentuk_dana: item.bentuk_dana };\r\n      this.showEdit = true;\r\n      this.confirmasi = false;\r\n      this.editItemId = item.detail; // Set the id of the item being edited\r\n\r\n      // Remove the axios.put call here\r\n    },\r\n    onSave() {\r\n      axios.put(`http://localhost:8000/api/bentukdana/${this.editItemId}`, {\r\n        bentuk_dana: this.form.bentuk_dana\r\n      }).then(res => {\r\n        console.log(res.data);\r\n        this.getbentuk_danas();\r\n        this.form.bentuk_dana = '';\r\n        this.showEdit = false;\r\n      }).catch(error => {\r\n        console.error(error);\r\n      });\r\n    },\r\n\r\n    deleteData(item) {\r\n      console.log(item);\r\n      this.confirmasiDelete = true;\r\n      this.deleteId = item.detail; // Set the id of the item being edited\r\n\r\n    },\r\n    onDelete() {\r\n      axios.delete(`http://localhost:8000/api/bentukdana/${this.deleteId}`)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.confirmasiDelete = false;\r\n          this.getbentuk_danas();\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n          this.confirmasiDelete = false;\r\n        });\r\n    },\r\n    remove(item, data) {\r\n      const index = data.indexOf(item.nama);\r\n      if (index >= 0) data.splice(index, 1);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<!-- methods: {\r\n  onSubmit(evt) {\r\n    evt.preventDefault();\r\n    alert(JSON.stringify(this.form));\r\n  },\r\n  onReset(evt) {\r\n    evt.preventDefault();\r\n    // Reset our form values\r\n    // Trick to reset/clear native browser form validation state\r\n    this.show = false;\r\n    this.$nextTick(() => {\r\n      this.show = true;\r\n    });\r\n  },\r\n  detailData(item) {\r\n    this.$router.push({\r\n      name: \"detail-penelitian\",\r\n      params: { data: item },\r\n    });\r\n  },\r\n  editData(item) {\r\n    console.log(\"test\");\r\n    this.showAdd = true;\r\n    this.form.bentukdana = item.bentukdana;\r\n  },\r\n  deleteData(item) {\r\n    this.confirmasiDelete = true;\r\n  },\r\n  addNew(value) {\r\n    this.showAdd = value;\r\n  },\r\n  onClick() {\r\n    this.confirmasi = true;\r\n  },\r\n\r\n  remove(item, data) {\r\n    const index = data.indexOf(item.nama);\r\n    if (index >= 0) data.splice(index, 1);\r\n  },\r\n}, -->"],"sourceRoot":"src/view/lppm/pages/com-master"}]}