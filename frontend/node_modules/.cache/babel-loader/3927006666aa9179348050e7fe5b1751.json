ncel\r\n                </v-btn>\r\n  \r\n                <v-btn\r\n                  color=\"green darken-1\"\r\n                  text\r\n                  @click=\"\r\n                    confirmasi = false;\r\n                    showAdd = false;\r\n                  \"\r\n                >\r\n                  Save\r\n                </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n          <v-dialog v-model=\"confirmasiDelete\" max-width=\"300\">\r\n            <v-card>\r\n              <v-card-title class=\"headline\"\r\n                >Apakah Anda Yakin Menghapus Role?</v-card-title\r\n              >\r\n  \r\n              <v-card-text>\r\n                pastikan anda menghapus role dengan benar\r\n              </v-card-text>\r\n  \r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n  \r\n                <v-btn\r\n                  color=\"green darken-1\"\r\n                  text\r\n                  @click=\"confirmasiDelete = false\"\r\n                >\r\n                  Cancel\r\n                </v-btn>\r\n  \r\n                <v-btn\r\n                  color=\"green darken-1\"\r\n                  text\r\n                  @click=\"\r\n                    confirmasiDelete = false;\r\n                    showAdd = false;\r\n                  \"\r\n                >\r\n                  Hapus\r\n                </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-row>\r\n        <v-dialog v-model=\"showAdd\" max-width=\"400px\">\r\n          <v-card class=\"\">\r\n            <v-toolbar flat>\r\n              <v-icon>mdi-file</v-icon>\r\n              <v-toolbar-title class=\"font-weight-light\">\r\n                Tambah Role\r\n              </v-toolbar-title>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"white\" fab small @click=\"showAdd = false\">\r\n                <v-icon> mdi-close </v-icon>\r\n              </v-btn>\r\n            </v-toolbar>\r\n            <v-divider></v-divider>\r\n            <v-card-text>\r\n              <v-form>\r\n                <v-row>\r\n                  <v-col md=\"6\">\r\n                    <v-text-field\r\n                      v-model=\"form.sumberdana\"\r\n                      label=\"Sumber Dana\"\r\n                      required\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n                <b-button\r\n                  type=\"button\"\r\n                  class=\"mr-2\"\r\n                  variant=\"primary\"\r\n                  @click=\"onClick()\"\r\n                  >Save</b-button\r\n                >\r\n              </v-form>\r\n            </v-card-text>\r\n          </v-card>\r\n        </v-dialog>\r\n        <CustomCard\r\n          title=\"Daftar Roles\"\r\n          deskripsi=\"List Role Yang Tersedia\"\r\n          :headers=\"headers\"\r\n          :contentTable=\"contentTables\"\r\n          :isDetail=\"false\"\r\n          @detailItem=\"detailData\"\r\n          @editItem=\"editData\"\r\n          @deleteItem=\"deleteData\"\r\n          @addNew=\"addNew\"\r\n        />\r\n      </div>\r\n    </b-card>\r\n  </template>\r\n  <script>\r\n  import { SET_BREADCRUMB } from \"@/core/services/store/breadcrumbs.module\";\r\n  import CustomCard from \"@/view/content/CustomCard\";\r\n  export default {\r\n    name: \"Sumberdana\",\r\n    components: {\r\n      CustomCard,\r\n    },\r\n    data() {\r\n      return {\r\n        form: {\r\n          sumberdana: \"\",\r\n        },\r\n  \r\n        semesters: [{ text: \"Select One\", value: null }, \"Genap\", \"Ganjil\"],\r\n        showAdd: false,\r\n        confirmasi: false,\r\n        confirmasiDelete: false,\r\n  \r\n        file: null,\r\n        headers: [\r\n          { text: \"No\", value: \"no\" },\r\n          { text: \"Role\", value: \"role\" },\r\n          { text: \"Actions\", value: \"detail\" },\r\n        ],\r\n        contentTables: [\r\n          {\r\n            no: \"1\",\r\n            role: \"LPPM\"\r\n          },\r\n  \r\n          {\r\n            no: \"2\",\r\n            role: \"Dosen\"\r\n          },\r\n\r\n          {\r\n            no: \"3\",\r\n            role: \"Kaprodi\"\r\n          },\r\n\r\n          {\r\n            no: \"4\",\r\n            role: \"Hrd\"\r\n          },\r\n\r\n          {\r\n            no: \"5\",\r\n            role: \"Team Reviewer\"\r\n          },\r\n        ],\r\n      };\r\n    },\r\n    methods: {\r\n      onSubmit(evt) {\r\n        evt.preventDefault();\r\n        alert(JSON.stringify(this.form));\r\n      },\r\n      onReset(evt) {\r\n        evt.preventDefault();\r\n        // Reset our form values\r\n        // Trick to reset/clear native browser form validation state\r\n        this.show = false;\r\n        this.$nextTick(() => {\r\n          this.show = true;\r\n        });\r\n      },\r\n      detailData(item) {\r\n        this.$router.push({\r\n          name: \"detail-penelitian\",\r\n          params: { data: item },\r\n        });\r\n      },\r\n      editData(item) {\r\n        console.log(\"test\");\r\n        this.showAdd = true;\r\n        this.form.sumberdana = item.sumberdana;\r\n      },\r\n      deleteData(item) {\r\n        this.confirmasiDelete = true;\r\n      },\r\n      addNew(value) {\r\n        this.showAdd = value;\r\n      },\r\n      onClick() {\r\n        this.confirmasi = true;\r\n      },\r\n  \r\n      remove(item, data) {\r\n        const index = data.indexOf(item.nama);\r\n        if (index >= 0) data.splice(index, 1);\r\n      },\r\n    },\r\n    mounted() {\r\n      this.$store.dispatch(SET_BREADCRUMB, [{ title: \"Master Data\" }]);\r\n    },\r\n  };\r\n  </script>"],"sourceRoot":"src/view/lppm/pages/com-master"}]}