A,OATA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,OAXA;AAYA,KA7DA;AA+DA,IAAA,UA/DA,sBA+DA,IA/DA,EA+DA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,WAAA,gBAAA,GAAA,IAAA;AACA,WAAA,QAAA,GAAA,IAAA,CAAA,MAAA,CAHA,CAGA;AAEA,KApEA;AAqEA,IAAA,QArEA,sBAqEA;AAAA;;AACA,MAAA,KAAA,CAAA,MAAA,oDAAA,KAAA,QAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,kBAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;AACA,OATA;AAUA,KAhFA;AAiFA,IAAA,MAjFA,kBAiFA,IAjFA,EAiFA,IAjFA,EAiFA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,IAAA,CAAA,EAAA,IAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AApFA;AApCA,CAAA","sourcesContent":["<template>\r\n  <b-card>\r\n    <div class=\"mt-3\">\r\n      <v-row justify=\"center\">\r\n        <v-dialog v-model=\"confirmasi\" max-width=\"300\">\r\n          <v-card>\r\n            <v-card-title class=\"headline\">Apakah Anda Yakin Ingin Menyimpan Data?</v-card-title>\r\n\r\n            <v-card-text> pastikan anda mengis data dengan benar </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"confirmasi = false\">\r\n                Cancel\r\n              </v-btn>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"onClick()\r\n              confirmasi = false;\r\n              showAdd = false;\r\n              \">\r\n                Save\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n        <v-dialog v-model=\"confirmasiDelete\" max-width=\"300\">\r\n          <v-card>\r\n            <v-card-title class=\"headline\">Apakah Anda Yakin Menghapus data?</v-card-title>\r\n\r\n            <v-card-text>\r\n              pastikan anda menghapus data dengan benar\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"confirmasiDelete = false\">\r\n                Cancel\r\n              </v-btn>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"onDelete();\r\n              confirmasiDelete = false;\r\n              showAdd = false;\r\n              \">\r\n                Hapus\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-row>\r\n      <v-dialog v-model=\"showAdd\" max-width=\"400px\">\r\n        <v-card class=\"\">\r\n          <v-toolbar flat>\r\n            <v-icon>mdi-file</v-icon>\r\n            <v-toolbar-title class=\"font-weight-light\">\r\n              Tambah Jenis Publikasi\r\n            </v-toolbar-title>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"white\" fab small @click=\"showAdd = false\">\r\n              <v-icon> mdi-close </v-icon>\r\n            </v-btn>\r\n          </v-toolbar>\r\n          <v-divider></v-divider>\r\n          <v-card-text>\r\n            <v-form>\r\n              <v-row>\r\n                <v-col md=\"6\">\r\n                  <v-text-field v-model=\"form.jenis_publikasi\" label=\"Jenis Publikasi\" required></v-text-field>\r\n                </v-col>\r\n                <v-col md=\"6\">\r\n                  <v-text-field v-model=\"form.bobot\" label=\"Bobot\" required></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n              <b-button type=\"button\" class=\"mr-2\" variant=\"primary\" @click=\"confirmasi = true;\">Save</b-button>\r\n            </v-form>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-dialog>\r\n      <v-dialog v-model=\"showEdit\" max-width=\"400px\">\r\n        <v-card class=\"\">\r\n          <v-toolbar flat>\r\n            <v-icon>mdi-file</v-icon>\r\n            <v-toolbar-title class=\"font-weight-light\">\r\n              Edit Jenis Publikasi\r\n            </v-toolbar-title>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"white\" fab small @click=\"showEdit = false\">\r\n              <v-icon> mdi-close </v-icon>\r\n            </v-btn>\r\n          </v-toolbar>\r\n          <v-divider></v-divider>\r\n          <v-card-text>\r\n            <v-form>\r\n              <v-row>\r\n                <v-col md=\"6\">\r\n                  <v-text-field v-model=\"form.jenis_publikasi\" label=\"Jenis Publikasi\" required></v-text-field>\r\n                </v-col>\r\n                <v-col md=\"6\">\r\n                  <v-text-field v-model=\"form.bobot\" label=\"Bobot\" required></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n              <b-button type=\"button\" class=\"mr-2\" variant=\"primary\" @click=\"onSave()\">Save</b-button>\r\n            </v-form>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-dialog>\r\n      <CustomCard title=\"Daftar Jenis Publikasi\" deskripsi=\"List Daftar Jenis Publikasi Yang Telah Di Inputkan\"\r\n        :headers=\"headers\" :contentTable=\"contentTables\" :isDetail=\"false\" @detailItem=\"detailData\" @editItem=\"editData\"\r\n        @deleteItem=\"deleteData\" @addNew=\"addNew\" />\r\n    </div>\r\n  </b-card>\r\n</template>\r\n<script>\r\nimport { SET_BREADCRUMB } from \"@/core/services/store/breadcrumbs.module\";\r\nimport CustomCard from \"@/view/content/CustomCard\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Jenispublikasi\",\r\n  components: {\r\n    CustomCard,\r\n  },\r\n  data() {\r\n    return {\r\n      form: {\r\n        jenis_publikasis: [],\r\n      },\r\n\r\n      semesters: [{ text: \"Select One\", value: null }, \"Genap\", \"Ganjil\"],\r\n      showEdit: false,\r\n      showAdd: false,\r\n      confirmasi: false,\r\n      confirmasiDelete: false,\r\n      contentTables: [],\r\n      file: null,\r\n      headers: [\r\n        { text: \"No\", value: \"no\" },\r\n        { text: \"Jenis Publikasi\", value: \"jenis_publikasi\" },\r\n        { text: \"Bobot\", value: \"bobot\" },\r\n        { text: \"Actions\", value: \"detail\" },\r\n      ],\r\n\r\n    };\r\n  },\r\n  computed:{\r\n    currentUser(){\r\n      return this.$store.state.auth.user.data\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getjenis_publikasi();\r\n    console.log(this.currentUser)\r\n  },\r\n  methods: {\r\n    getjenis_publikasi() {\r\n      axios.get('http://localhost:8000/api/jenispublikasi').then(res => {\r\n        this.contentTables = res.data.data.map((bf, index) => {\r\n          return {\r\n            no: index + 1,\r\n            jenis_publikasi: bf.jenis_publikasi,\r\n            bobot: bf.bobot,\r\n            detail: bf.id\r\n          }\r\n        })\r\n        console.log()\r\n      })\r\n    },\r\n\r\n    addNew(value) {\r\n      console.log(value)\r\n      this.showAdd = value;\r\n    },\r\n    onClick() {\r\n      axios.post('http://localhost:8000/api/jenispublikasi', {\r\n        jenis_publikasi: this.form.jenis_publikasi,\r\n        bobot: this.form.bobot,\r\n      })\r\n        .then(response => {\r\n          // this.confirmasi = true;\r\n          this.getjenis_publikasi();\r\n          this.form.jenis_publikasi = '';\r\n          this.form.bobot = '';\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    detailData(item) {\r\n      this.$router.push({\r\n        name: \"detail-penelitian\",\r\n        params: { data: item },\r\n      });\r\n    },\r\n    editData(item) {\r\n      console.log(item);\r\n      this.form = { jenis_publikasi: item.jenis_publikasi, bobot: item.bobot };\r\n      this.showEdit = true;\r\n      this.confirmasi = false;\r\n      this.editItemId = item.detail; // Set the id of the item being edited\r\n\r\n    },\r\n    onSave() {\r\n      axios.put(`http://localhost:8000/api/jenispublikasi/${this.editItemId}`, {\r\n        jenis_publikasi: this.form.jenis_publikasi,\r\n        bobot: this.form.bobot,\r\n      }).then(res => {\r\n        console.log(res.data);\r\n        this.getjenis_publikasi();\r\n        this.form.jenis_publikasi = '';\r\n        this.form.bobot = '';\r\n        this.showEdit = false;\r\n      }).catch(error => {\r\n        console.error(error);\r\n      });\r\n    },\r\n\r\n    deleteData(item) {\r\n      console.log(item);\r\n      this.confirmasiDelete = true;\r\n      this.deleteId = item.detail; // Set the id of the item being edited\r\n\r\n    },\r\n    onDelete() {\r\n      axios.delete(`http://localhost:8000/api/jenispublikasi/${this.deleteId}`)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.confirmasiDelete = false;\r\n          this.getjenis_publikasi();\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n          this.confirmasiDelete = false;\r\n        });\r\n    },\r\n    remove(item, data) {\r\n      const index = data.indexOf(item.nama);\r\n      if (index >= 0) data.splice(index, 1);\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n// methods: {\r\n//   onSubmit(evt) {\r\n//     evt.preventDefault();\r\n//     alert(JSON.stringify(this.form));\r\n//   },\r\n//   onReset(evt) {\r\n//     evt.preventDefault();\r\n//     // Reset our form values\r\n//     // Trick to reset/clear native browser form validation state\r\n//     this.show = false;\r\n//     this.$nextTick(() => {\r\n//       this.show = true;\r\n//     });\r\n//   },\r\n//   detailData(item) {\r\n//     this.$router.push({\r\n//       name: \"detail-penelitian\",\r\n//       params: { data: item },\r\n//     });\r\n//   },\r\n//   editData(item) {\r\n//     console.log(\"test\");\r\n//     this.showAdd = true;\r\n//     this.form.jenispublikasi = item.jenispublikasi;\r\n//     this.form.bobot = item.bobot;\r\n//   },\r\n//   deleteData(item) {\r\n//     this.confirmasiDelete = true;\r\n//   },\r\n//   addNew(value) {\r\n//     this.showAdd = value;\r\n//   },\r\n//   onClick() {\r\n//     this.confirmasi = true;\r\n//   },\r\n\r\n//   remove(item, data) {\r\n//     const index = data.indexOf(item.nama);\r\n//     if (index >= 0) data.splice(index, 1);\r\n//   },\r\n// },"],"sourceRoot":"src/view/lppm/pages"}]}