          <td>\r\n                    <h5>: {{ dosen.nama }}</h5>\r\n                  </td>\r\n                </tr>\r\n                <tr :key=\"index + '_' + dosen.nidn\">\r\n                  <td>\r\n                    <h5 class=\"ml-3\">NIDN</h5>\r\n                  </td>\r\n                  <td>\r\n                    <h5>: {{ dosen.nidn }}</h5>\r\n                  </td>\r\n                </tr>\r\n                <tr :key=\"index + '_' + dosen.jabatanFungsional\">\r\n                  <td>\r\n                    <h5 class=\"ml-3\">Jabatan Fungsional</h5>\r\n                  </td>\r\n                  <td>\r\n                    <h5>: {{ dosen.jabatanFungsional }}</h5>\r\n                  </td>\r\n                </tr>\r\n                <tr :key=\"index + '_' + dosen.programStudi\">\r\n                  <td>\r\n                    <h5 class=\"ml-3\">Program Studi</h5>\r\n                  </td>\r\n                  <td>\r\n                    <h5>: {{ dosen.programStudi }}</h5>\r\n                  </td>\r\n                </tr>\r\n                <tr :key=\"index + '_' + dosen.noHPEmail\">\r\n                  <td>\r\n                    <h5 class=\"ml-3\">No.Hp/Email</h5>\r\n                  </td>\r\n                  <td>\r\n                    <h5>: {{ dosen.noHPEmail }}</h5>\r\n                  </td>\r\n                </tr>\r\n              </template>\r\n\r\n              <template\r\n                v-for=\"(Mahasiswa, index) in dataLaporan.anggotaMahasiswa\"\r\n              >\r\n                <tr class=\"mt-2\" :key=\"index + '_' + Mahasiswa.nama\">\r\n                  <td>\r\n                    <h5>Anggota Mahasiswa {{ index + 1 }}</h5>\r\n                  </td>\r\n                  <td>\r\n                    <h5>: {{ Mahasiswa.nama }}</h5>\r\n                  </td>\r\n                </tr>\r\n                <tr :key=\"index + '_' + Mahasiswa.nim\">\r\n                  <td>\r\n                    <h5 class=\"ml-3\">NIM</h5>\r\n                  </td>\r\n                  <td>\r\n                    <h5>: {{ Mahasiswa.nim }}</h5>\r\n                  </td>\r\n                </tr>\r\n                <tr :key=\"index + '_' + Mahasiswa.prodi\">\r\n                  <td>\r\n                    <h5 class=\"ml-3\">Prodi</h5>\r\n                  </td>\r\n                  <td>\r\n                    <h5>: {{ Mahasiswa.prodi }}</h5>\r\n                  </td>\r\n                </tr>\r\n              </template>\r\n              <tr class=\"mt-2\">\r\n                <td><h5>Bidang Fokus</h5></td>\r\n                <td>\r\n                  <h5>: {{ dataLaporan.bidangFokus }}</h5>\r\n                </td>\r\n              </tr>\r\n              <tr>\r\n                <td><h5>Mitra</h5></td>\r\n                <td>\r\n                  <h5>: {{ dataLaporan.mitra }}</h5>\r\n                </td>\r\n              </tr>\r\n              <template v-for=\"(dana, index) in dataLaporan.dana\">\r\n                <tr :key=\"index + '_sumberdana'\">\r\n                  <td><h5>Sumber Dana</h5></td>\r\n                  <td>\r\n                    <h5>: {{ dana.sumberDana }}</h5>\r\n                  </td>\r\n                </tr>\r\n                <tr :key=\"index + '_bentukdana'\">\r\n                  <td><h5>Bentuk Dana</h5></td>\r\n                  <td>\r\n                    <h5>: {{ dana.bentukDana }}</h5>\r\n                  </td>\r\n                </tr>\r\n                <tr :key=\"index + '_jumlahdana'\">\r\n                  <td><h5>Jumlah Dana</h5></td>\r\n                  <td>\r\n                    <h5>: Rp.{{ dana.jumlahDana }}</h5>\r\n                  </td>\r\n                </tr>\r\n              </template>\r\n              <tr class=\"mt-2\">\r\n                <td><h5>Laporan Proposal</h5></td>\r\n                <td>\r\n                  <h5>\r\n                    <a\r\n                      v-if=\"dataLaporan.fileNameProposal !== ''\"\r\n                      :href=\"'./berkas/' + dataLaporan.fileNameProposal\"\r\n                      target=\"__blank\"\r\n                    >\r\n                      :\r\n                      <i class=\"flaticon-file-2 text-primary\"></i>\r\n                      {{ dataLaporan.fileNameKemajuan }}\r\n                    </a>\r\n                    <div v-else>: File Tidak Tersedia</div>\r\n                  </h5>\r\n                </td>\r\n              </tr>\r\n              <tr class=\"mt-2\">\r\n                <td><h5>Laporan Kemajuan</h5></td>\r\n                <td>\r\n                  <h5>\r\n                    <a\r\n                      v-if=\"dataLaporan.fileNameKemajuan !== ''\"\r\n                      :href=\"'./berkas/' + dataLaporan.fileNameKemajuan\"\r\n                      target=\"__blank\"\r\n                    >\r\n                      :\r\n                      <i class=\"flaticon-file-2 text-primary\"></i>\r\n                      {{ dataLaporan.fileNameKemajuan }}\r\n                    </a>\r\n                    <div v-else>: File Tidak Tersedia</div>\r\n                  </h5>\r\n                </td>\r\n              </tr>\r\n              <tr class=\"mt-2\">\r\n                <td><h5>Laporan Akhir</h5></td>\r\n                <td>\r\n                  <h5>\r\n                    <a\r\n                      v-if=\"dataLaporan.fileNameAkhir !== ''\"\r\n                      :href=\"'./berkas/' + dataLaporan.fileNameAkhir\"\r\n                      target=\"__blank\"\r\n                    >\r\n                      :\r\n                      <i class=\"flaticon-file-2 text-primary\"></i>\r\n                      {{ dataLaporan.fileNameKemajuan }}\r\n                    </a>\r\n                    <div v-else>: File Tidak Tersedia</div>\r\n                  </h5>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </v-form>\r\n      </v-card-text>\r\n    </v-card>\r\n    <v-card class=\"mt-2\">\r\n      <v-card-title> Publikasi</v-card-title>\r\n      <ListPublikasi :dataList=\"datalistPublikasi\"></ListPublikasi>\r\n    </v-card>\r\n    <v-card>\r\n      <v-card-title>\r\n        Hasil Penilaian Input\r\n        <v-spacer></v-spacer>\r\n      </v-card-title>\r\n      <v-data-table\r\n        :headers=\"headerPenilaianInput\"\r\n        :items=\"dataNilaiInput\"\r\n        hide-default-footer\r\n      >\r\n        <template slot=\"item\" slot-scope=\"props\">\r\n          <template v-for=\"(indikator, i) in props.item.indikators\">\r\n            <tr :key=\"i\">\r\n              <td\r\n                class=\"text-start\"\r\n                v-if=\"i == 0\"\r\n                :rowspan=\"props.item.indikators.length\"\r\n              >\r\n                {{ props.item.no }}\r\n              </td>\r\n              <td\r\n                class=\"text-start\"\r\n                v-if=\"i == 0\"\r\n                :rowspan=\"props.item.indikators.length\"\r\n              >\r\n                {{ props.item.kriteria }}\r\n              </td>\r\n              <td>{{ indikator.name }}</td>\r\n              <td>{{ indikator.bobot }}</td>\r\n              <td>\r\n                {{ indikator.nilai }}\r\n              </td>\r\n              <!-- <v-text-field v-model=\"item.nilai\" type=\"number\"></v-text-field> -->\r\n            </tr>\r\n          </template>\r\n        </template>\r\n\r\n        <template slot=\"body.append\">\r\n          <tr>\r\n            <th colspan=\"4\" class=\"text-end\">Total</th>\r\n            <th>\r\n              {{ getTotalValue(dataNilaiInput) }}\r\n            </th>\r\n          </tr>\r\n        </template>\r\n      </v-data-table>\r\n    </v-card>\r\n    <v-card>\r\n      <v-card-title>\r\n        Hasil Penilaian Output\r\n        <v-spacer></v-spacer>\r\n      </v-card-title>\r\n      <v-data-table\r\n        :headers=\"headerPenilaianOutput\"\r\n        :items=\"dataNilaiOutput\"\r\n        hide-default-footer\r\n      >\r\n        <template slot=\"item\" slot-scope=\"props\">\r\n          <template v-for=\"(indikator, i) in props.item.indikators\">\r\n            <tr :key=\"i\">\r\n              <td\r\n                class=\"text-start\"\r\n                v-if=\"i == 0\"\r\n                :rowspan=\"props.item.indikators.length\"\r\n              >\r\n                {{ props.item.no }}\r\n              </td>\r\n              <td\r\n                class=\"text-start\"\r\n                v-if=\"i == 0\"\r\n                :rowspan=\"props.item.indikators.length\"\r\n              >\r\n                {{ props.item.kriteria }}\r\n              </td>\r\n              <td>{{ indikator.name }}</td>\r\n              <td>{{ indikator.bobot }}</td>\r\n              <td>\r\n                {{ indikator.nilai }}\r\n              </td>\r\n              <!-- <v-text-field v-model=\"item.nilai\" type=\"number\"></v-text-field> -->\r\n            </tr>\r\n          </template>\r\n        </template>\r\n\r\n        <template slot=\"body.append\">\r\n          <tr>\r\n            <th colspan=\"4\" class=\"text-end\">Total</th>\r\n            <th>\r\n              {{ getTotalValue(dataNilaiOutput) }}\r\n            </th>\r\n          </tr>\r\n        </template>\r\n      </v-data-table>\r\n    </v-card>\r\n    <v-card>\r\n      <v-card-title>\r\n        Hasil Penilaian Final\r\n        <v-spacer></v-spacer>\r\n      </v-card-title>\r\n      <v-data-table\r\n        :headers=\"headerPenilaianFinal\"\r\n        :items=\"dataNilaiFinal\"\r\n        hide-default-footer\r\n      >\r\n        <template slot=\"item\" slot-scope=\"props\">\r\n          <template v-for=\"(indikator, i) in props.item.indikators\">\r\n            <tr :key=\"i\">\r\n              <td\r\n                class=\"text-start\"\r\n                v-if=\"i == 0\"\r\n                :rowspan=\"props.item.indikators.length\"\r\n              >\r\n                {{ props.item.no }}\r\n              </td>\r\n              <td\r\n                class=\"text-