my-2 mr-5\">\r\n          {{ title }}\r\n        </h5>\r\n        <ul\r\n          class=\"\r\n            breadcrumb breadcrumb-transparent breadcrumb-dot\r\n            font-weight-bold\r\n            p-0\r\n            my-2\r\n          \"\r\n        >\r\n          <li class=\"breadcrumb-item\">\r\n            <router-link :to=\"''\" class=\"subheader-breadcrumbs-home\">\r\n              <i class=\"flaticon2-shelter text-muted icon-1x\"></i>\r\n            </router-link>\r\n          </li>\r\n\r\n          <template v-for=\"(breadcrumb, i) in breadcrumbs\">\r\n            <li class=\"breadcrumb-item\" :key=\"`${i}-${breadcrumb.id}`\">\r\n              <router-link\r\n                v-if=\"breadcrumb.route\"\r\n                :key=\"i\"\r\n                :to=\"breadcrumb.route\"\r\n                class=\"text-muted\"\r\n              >\r\n                {{ breadcrumb.title }}\r\n              </router-link>\r\n              <span class=\"text-muted\" :key=\"i\" v-if=\"!breadcrumb.route\">\r\n                {{ breadcrumb.title }}\r\n              </span>\r\n            </li>\r\n          </template>\r\n        </ul>\r\n      </div>\r\n      <div class=\"text-center\" v-if=\"status\">\r\n        <v-chip\r\n          class=\"ma-2\"\r\n          :color=\"checkStatus(status).buttonColor\"\r\n          label\r\n          text-color=\"white\"\r\n        >\r\n          <v-icon left>{{ checkStatus(status).icon }}</v-icon>\r\n          {{ status }}\r\n        </v-chip>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style lang=\"scss\">\r\n.custom-v-dropdown {\r\n  &.dropdown-toggle {\r\n    padding: 0;\r\n    &:hover {\r\n      text-decoration: none;\r\n    }\r\n\r\n    &.dropdown-toggle-no-caret {\r\n      &:after {\r\n        content: none;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.dropdown-menu {\r\n    margin: 0;\r\n    padding: 0;\r\n    outline: none;\r\n    .b-dropdown-text {\r\n      padding: 0;\r\n    }\r\n  }\r\n}\r\n</style>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"KTSubheader\",\r\n  props: {\r\n    breadcrumbs: Array,\r\n    title: String,\r\n    status: String,\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"layoutConfig\"]),\r\n\r\n    /**\r\n     * Check if subheader width is fluid\r\n     */\r\n    widthFluid() {\r\n      return this.layoutConfig(\"subheader.width\") === \"fluid\";\r\n    },\r\n\r\n    subheaderClasses() {\r\n      const classes = [];\r\n      const style = this.layoutConfig(\"subheader.style\");\r\n      if (style) {\r\n        classes.push(style);\r\n\r\n        if (style === \"solid\") {\r\n          classes.push(\"bg-white\");\r\n        }\r\n\r\n        if (this.layoutConfig(\"subheader.fixed\")) {\r\n          classes.push(\"border-top\");\r\n        }\r\n      }\r\n      return classes.join(\" \");\r\n    },\r\n  },\r\n  methods: {\r\n    checkStatus(status) {\r\n      if (status == \"success\") {\r\n        return {\r\n          icon: \"mdi-check-circle-outline\",\r\n          buttonColor: \"green lighten-1\",\r\n        };\r\n      } else if (status == \"rejected\") {\r\n        return {\r\n          icon: \"mdi-close-cirle-outline\",\r\n          buttonColor: \"light-red lighten-1\",\r\n        };\r\n      } else if (status == \"empty\") {\r\n        return {\r\n          icon: \"mdi-plus-circle-outline\",\r\n          buttonColor: \"light-blue lighten-1\",\r\n        };\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}