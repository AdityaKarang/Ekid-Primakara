n                  <td>\r\n                    <h6>Anggota Mahasiswa {{ index + 1 }}</h6>\r\n                  </td>\r\n                  <td>\r\n                    <h6>: {{ mahasiswa.name }}</h6>\r\n                  </td>\r\n                </tr>\r\n                <tr>\r\n                  <td>\r\n                    <h6 class=\"ml-3\">NIM</h6>\r\n                  </td>\r\n                  <td>\r\n                    <h6>: {{ mahasiswa.nim }}</h6>\r\n                  </td>\r\n                </tr>\r\n                <tr>\r\n                  <td>\r\n                    <h6 class=\"ml-3\">Program Studi</h6>\r\n                  </td>\r\n                  <td>\r\n                    <h6>: {{ mahasiswa.prodi }}</h6>\r\n                  </td>\r\n                </tr>\r\n              </template>\r\n              <tr class=\"mt-2\">\r\n                <td>\r\n                  <h5>Bidang Fokus</h5>\r\n                </td>\r\n                <td>\r\n                  <h5>: {{ dataLaporan.bidang_fokus }}</h5>\r\n                </td>\r\n              </tr>\r\n              <tr>\r\n                <td>\r\n                  <h5>Mitra</h5>\r\n                </td>\r\n                <td>\r\n                  <h5>: {{ dataLaporan.mitra }}</h5>\r\n                </td>\r\n              </tr>\r\n              <tr>\r\n                <td>\r\n                  <h6>Sumber Dana</h6>\r\n                </td>\r\n                <td>\r\n                  <h6>: {{ dataLaporan.sumber_dana }}</h6>\r\n                </td>\r\n              </tr>\r\n              <tr>\r\n                <td>\r\n                  <h6>Bentuk Dana</h6>\r\n                </td>\r\n                <td>\r\n                  <h6>: {{ dataLaporan.bentuk_dana }}</h6>\r\n                </td>\r\n              </tr>\r\n              <tr>\r\n                <td>\r\n                  <h6>Jumlah Dana</h6>\r\n                </td>\r\n                <td>\r\n                  <h6>: Rp.{{ dataLaporan.jumlah_dana }}