ions\r\n     * @returns {null|*}\r\n     */\r\n    headerMenuClasses() {\r\n      const classes = this.getClasses(\"header_menu\");\r\n      if (typeof classes !== \"undefined\") {\r\n        return classes.join(\" \");\r\n      }\r\n      return null;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}